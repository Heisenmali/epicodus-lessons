<h1 class="pl3">Products</h1>



<div class="fl w-10 pl3">
  <% @products.each do |product|%>
    <div class="pv2">
      <%= link_to product.name, product_path(product), class: "" %>
    </div>
  <% end %>
</div>

<div class="fl w-50 ml6"> 
  <%= render :partial => "show"  if action_name == "show" %>
</div>
