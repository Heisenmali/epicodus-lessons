<h1>To Do List</h1>

<% @tasks.each do |task| %>
<li><%= task.description %></li>
<% end %>

<button id="new-task" type="button" class="btn btn-default">New task!</button>

<form class="new-task" action="/tasks/new" method="post">
  <label for="description">Putt stuff in!</label>
  <input type="text" name="description">
</form>
